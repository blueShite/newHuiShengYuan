<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/appwhite"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/text_address_list_item_set"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="19dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="编辑"
        android:textSize="@dimen/smallText"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/text_address_list_item_delete"
        app:layout_constraintTop_toBottomOf="@+id/textView46"/>

    <TextView
        android:id="@+id/textView41"
        android:layout_width="0dp"
        android:background="@color/ColorBorBlack"
        android:layout_height="10dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/button_address_list_item_select"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:enabled="false"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/address_list_select"
        app:layout_constraintBottom_toTopOf="@+id/textView46"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView41"/>

    <TextView
        android:id="@+id/text_address_list_item_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="@dimen/mainText"
        android:textColor="@color/ColorBlcak"
        app:layout_constraintLeft_toRightOf="@+id/button_address_list_item_select"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView41"/>

    <TextView
        android:id="@+id/text_address_list_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textColor="@color/ColorBlcak"
        android:textSize="@dimen/mainText"
        app:layout_constraintLeft_toRightOf="@+id/text_address_list_item_name"
        app:layout_constraintTop_toBottomOf="@+id/textView41"
        />

    <TextView
        android:id="@+id/text_address_list_item_address"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="@dimen/smallText"
        app:layout_constraintLeft_toRightOf="@+id/button_address_list_item_select"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/text_address_list_item_name"
        app:layout_constraintHorizontal_bias="0.0"/>

    <TextView
        android:id="@+id/textView46"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:background="@color/ColorLine"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_address_list_item_address"/>

    <TextView
        android:id="@+id/text_address_list_item_delete"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginRight="16dp"
        android:gravity="center"
        android:text="删除"
        android:textSize="@dimen/smallText"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView46"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"/>

    <TextView
        android:id="@+id/text_address_list_select"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/text_address_list_item_address"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/text_address_list_phone"/>
</android.support.constraint.ConstraintLayout>